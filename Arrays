#include <iostream>
#include <bits/stdc++.h>
using namespace std;

int maxx(int arr[],int s){
    int i;
    int low = arr[0];
    for(i=0;i<s;i++){
        low = max(low,arr[i]);
    }
    cout<<low<<"\n";
}

int minn(int arr[],int s){
    int i;
    int high = arr[0];
    for(i=0;i<s;i++){
        high = min(high,arr[i]);
    }
    cout<<high<<"\n";
}

int main() {
   int n;
   cin>>n;
   int arr[n]={0};
   for(int i=0;i<n;i++){
       cin>>arr[i];
   }
   maxx(arr,n);
   minn(arr,n);
}
